include "std"
class Op
  id:int
  type:int
  token:Token
  func:Func

  method init
    id:int
    type:int
    token:Token
    func:Func
  -> Op :
    Op.size malloc cast(Op)
    take op in

    id              op Op->id
    type            op Op->type
    token           op Op->token
    func            op Op->func
    op
  end

  method load ptr -> Op :
    ptr.load cast(Op)
  end
endclass
